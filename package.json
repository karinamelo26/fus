    "jsdom": "~20.0.0",
    "lint-staged": "~13.0.3",
    "mssql": "~8.1.2",
    "mysql": "~2.18.1",
    "prettier": "~2.7.1",
    "prettier-plugin-prisma": "~4.1.0",
    "prisma": "~4.1.1",
    "react": "~18.2.0",
    "react-dom": "~18.2.0",
    "react-hook-form": "~7.34.0",
    "react-router-dom": "~6.3.0",
    "reflect-metadata": "~0.1.13",
    "sass": "~1.54.0",
