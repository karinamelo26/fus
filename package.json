    "@fontsource/roboto": "~4.5.7",
    "@mui/icons-material": "~5.8.4",
    "@mui/material": "~5.9.3",
    "@tanstack/react-query": "~4.0.10",
    "@testing-library/react": "~13.3.0",
    "@types/cron": "~2.0.0",
    "@types/mssql": "~8.0.3",
    "@types/mysql": "~2.15.21",
    "@types/node": "~18.6.3",
    "@types/react": "~18.0.15",
