    "@typescript-eslint/eslint-plugin": "~5.32.0",
    "@typescript-eslint/parser": "~5.32.0",
    "@vitejs/plugin-react": "~2.0.0",
    "@vitest/ui": "~0.19.1",
    "c8": "~7.12.0",
    "camelcase": "~6.3.0",
    "class-transformer": "~0.5.1",
    "class-validator": "~0.13.2",
    "cron": "~2.1.0",
    "cross-env": "~7.0.3",
    "date-fns": "~2.29.1",
    "dotenv": "~16.0.1",
    "electron": "~20.0.0",
    "electron-builder": "~23.3.3",
